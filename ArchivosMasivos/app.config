<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
    </configSections>
	<appSettings>
		<!--Asunto del mail que se va a enviar personalizado-->
		<add key="Asunto" value="Acá se escribe el asunto del mail" />
		<!--Mensaje del mail que se va a enviar personalizado-->
		<add key="Mensaje" value="En este espacio se escribe el mensaje correspondiente" />
		<!--Asunto para mail que se va a enviar con multiples destinos-->
		<add key="AsuntoMasivo" value="Acá se escribe el asunto del mail cuando se va a enviar masivo" />
		<!--Mensaje para mail que se va a enviar con multiples destinos-->
		<add key="MensajeMasivo" value="En este espacio se escribe el mensaje correspondiente cuando se va a enviar masivo" />
		<!--Seleccionar en que ruta se crearan los archivos 0 -> Mis documentos     1 -> Carpeta desde donde se ejecuta la aplicación-->
		<add key="CarpetaApp" value="1" />
		<!--Cuenta configurada en outlook que se usará para el envío del correo-->
		<add key="CuentaAsociada" value="pasivocol@minhacienda.gov.co" />
		<add key="Setting" value="true" />
	</appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" /></startup>
